{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/dist/(.*)",
      "destination": "/dist/$1"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "maternidad.avasalud.mx"
        }
      ],
      "destination": "/landing-maternidad/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
  